{
  "name": "verus-connect",
  "version": "0.1.0",
  "description": "Drop-in VerusID login for any website â€” server middleware + frontend SDK",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "verus",
    "verusid",
    "crypto",
    "wallet",
    "login",
    "blockchain",
    "authentication"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.25",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "qrcode-generator": "^1.4.4"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "verusid-ts-client": "*"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "verusid-ts-client": {
      "optional": true
    }
  }
}
